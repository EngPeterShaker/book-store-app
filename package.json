{
  "name": "book-store-app",
  "version": "1.0.0",
  "description": "Full-stack book store application with NestJS backend and React frontend",
  "private": true,
  "packageManager": "yarn@1.22.22",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "scripts": {
    "dev": "turbo dev",
    "build": "turbo build",
    "test": "turbo test",
    "lint": "turbo lint",
    "clean": "turbo clean && rm -rf node_modules",
    "install:all": "yarn install"
  },
  "keywords": [
    "nestjs",
    "react",
    "react-native",
    "typescript",
    "postgresql",
    "vercel",
    "fullstack",
    "monorepo",
    "turborepo"
  ],
  "author": "Your Name",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.2",
    "turbo": "^2.6.1"
  }
}
