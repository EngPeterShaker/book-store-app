    "FRONTEND_URL": "https://book-store-mq7vytzeq-engpetershakers-projects.vercel.app"
  },
  "installCommand": "corepack yarn install",
  "buildCommand": "turbo build --filter=@book-store/web --filter=@book-store/api"
}
