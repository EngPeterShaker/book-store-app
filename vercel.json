{
  "version": 2,
  "builds": [
    {
      "src": "apps/web/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    },
    {
      "src": "apps/api/src/main.vercel.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/apps/api/src/main.vercel.ts"
    },
    {
      "src": "/static/(.*)",
      "dest": "/apps/web/static/$1"
    },
    {
      "src": "/(.*\\.(js|css|png|jpg|jpeg|gif|ico|svg))",
      "dest": "/apps/web/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/apps/web/index.html"
    }
  ],
  "env": {
    "NODE_ENV": "production",
    "DB_DISABLED": "false",
