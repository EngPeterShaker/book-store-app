{
  "version": 2,
  "buildCommand": "yarn vercel-build",
  "installCommand": "yarn install:all",
  "outputDirectory": "frontend/build",
  "functions": {
    "api/[...].ts": {
      "maxDuration": 30
    }
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/[...]"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}